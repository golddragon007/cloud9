[common]
server_addr = {{ salt['pillar.get']('frp:host') }}
server_port = {{ salt['pillar.get']('frp:port') }}
privilege_token = {{ salt['pillar.get']('frp:token') }}

[{{ frp_name }}]
type = http
local_ip = 127.0.0.1
local_port = 8080
use_encryption = false
use_compression = true
subdomain = {{ frp_name }}
